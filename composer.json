{
    "name": "ongr/router-bundle",
    "description": "Bundle for ONGR routing.",
    "keywords": ["router", "seo", "ongr"],
    "type": "symfony-bundle",
    "homepage": "http://ongr.io",
    "license": "MIT",
    "authors": [
        {
            "name": "ONGR team",
            "homepage": "http://www.ongr.io"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "~2.7|~3.0",
        "symfony-cmf/routing": "^1.4.0",
        "ongr/elasticsearch-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "symfony/monolog-bundle": "~2.3|~3.0",
        "squizlabs/php_codesniffer": "~2.0",
        "satooshi/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": { "ONGR\\RouterBundle\\": "" },
        "exclude-from-classmap": ["/Tests/"]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
